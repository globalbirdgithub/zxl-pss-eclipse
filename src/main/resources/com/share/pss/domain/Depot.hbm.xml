<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.share.pss.domain">
	<class name="Depot" table="depot">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" column="name"/>
		<property name="maxCapacity" column="maxCapacity"/>
		<property name="currentCapacity" column="currentCapacity"/>
		<property name="totalAmount" column="totalAmount"/>
	</class>
</hibernate-mapping>